<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.grocerylist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GroceryList">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAUSLJ8i-alhKTyrLN1qIWKXil31MqfZrE" />

        <activity android:name=".ItemInfoActivity"
            android:parentActivityName=".GroceryListActivity">
        </activity>
        <activity android:name=".GroceryListActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".NearbyGroceryStoresActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".PantryActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>

</manifest>